{
  "name": "@elizaos/plugin-itoro-bridge",
  "description": "Bridge plugin for connecting ElizaOS with ITORO multi-agent trading system",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "packageType": "plugin",
  "platform": "node",
  "license": "MIT",
  "keywords": [
    "plugin",
    "elizaos",
    "itoro",
    "trading",
    "bridge"
  ],
  "homepage": "https://elizaos.ai",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    ".npmignore",
    ".gitignore",
    "package.json"
  ],
  "dependencies": {
    "@elizaos/core": "workspace:*",
    "zod": "3.24.2",
    "@supabase/supabase-js": "^2.39.0",
    "redis": "^4.6.0",
    "better-sqlite3": "^9.0.0",
    "node-fetch": "^3.3.0"
  },
  "devDependencies": {
    "@elizaos/cli": "workspace:*",
    "prettier": "3.5.3",
    "typescript": "5.8.2"
  },
  "scripts": {
    "build": "bun run build.ts",
    "lint": "prettier --write ./src",
    "format": "prettier --write ./src",
    "format:check": "prettier --check ./src",
    "build:watch": "bun run build.ts --watch"
  },
  "resolutions": {
    "zod": "3.24.2"
  }
}

